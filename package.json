{
  "name": "@lawnstarter/react-native-ui-datepicker",
  "version": "1.4.0",
  "type": "module",
  "description": "Customizable date picker for React Native",
  "types": "dist/index.d.ts",
  "main": "./dist/react-native-ui-datepicker.cjs",
  "module": "./dist/react-native-ui-datepicker.js",
  "react-native": "./dist/react-native-ui-datepicker.js",
  "source": "src/index",
  "files": [
    "src",
    "dist",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "scripts": {
    "cm": "git cz",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "build": "vite build",
    "prepublish": "cp -r package.json ./dist"
  },
  "keywords": [
    "react-native",
    "ios",
    "android",
    "react-native-ui-datetpicker",
    "react-native-datetime",
    "react-native-datetime-picker",
    "react-native-datetpicker",
    "react-native-datet-picker",
    "react-native-timepicker",
    "react-native-time-picker",
    "react-native-calendar",
    "react-native-daterange",
    "datetime",
    "datetime-picker",
    "datepicker",
    "datet-picker",
    "timepicker",
    "time-picker",
    "calendar",
    "daterange"
  ],
  "repository": "https://github.com/lawnstarter/react-native-ui-datepicker",
  "author": "Farhoud Shapouran <shapouran@gmail.com> (https://github.com/farhoudshapouran)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lawnstarter/react-native-ui-datepicker/issues"
  },
  "homepage": "https://github.com/lawnstarter/react-native-ui-datepicker#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "exports": {
    ".": {
      "import": "./dist/react-native-ui-datepicker.js",
      "require": "./dist/react-native-ui-datepicker.cjs",
      "types": "./dist/index.d.ts"
    },
    "./types": {
      "types": "./dist/react-native-ui-datepicker/src/types.d.ts",
      "require": "./dist/react-native-ui-datepicker/types.cjs",
      "import": "./dist/react-native-ui-datepicker/types.js",
      "react-native": "./dist/react-native-ui-datepicker/types.js"
    },
    "./enums": {
      "types": "./dist/react-native-ui-datepicker/src/enums.d.ts",
      "require": "./dist/react-native-ui-datepicker/enums.cjs",
      "import": "./dist/react-native-ui-datepicker/enums.js",
      "react-native": "./dist/react-native-ui-datepicker/enums.js"
    },
    "./utils": {
      "types": "./dist/react-native-ui-datepicker/src/utils.d.ts",
      "require": "./dist/react-native-ui-datepicker/utils.cjs",
      "import": "./dist/react-native-ui-datepicker/utils.js",
      "react-native": "./dist/react-native-ui-datepicker/utils.js"
    },
    "./assets/*": "./dist/assets/*"
  },
  "typesVersions": {
    "*": {
      "types": [
        "./dist/react-native-ui-datepicker/src/types.d.ts"
      ],
      "enums": [
        "./dist/react-native-ui-datepicker/src/enums.d.ts"
      ],
      "utils": [
        "./dist/react-native-ui-datepicker/src/utils.d.ts"
      ]
    }
  },
  "devDependencies": {
    "@evilmartians/lefthook": "^1.2.2",
    "@react-native-community/eslint-config": "^3.0.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^11.1.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.2.6",
    "@semantic-release/npm": "^11.0.2",
    "@semantic-release/release-notes-generator": "^12.1.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.3.2",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.14.202",
    "@types/react": "18.2.0",
    "@types/react-native": "0.72.0",
    "@types/react-test-renderer": "^18.0.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "commitizen": "^4.3.0",
    "commitlint": "^17.0.2",
    "cz-ls-commits": "^1.1.0",
    "del-cli": "^5.0.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^28.1.3",
    "pod-install": "^0.1.0",
    "prettier": "^2.0.5",
    "react": "18.2.0",
    "react-native": "0.72.0",
    "react-native-builder-bob": "^0.20.0",
    "react-test-renderer": "18.2.0",
    "semantic-release": "^22.0.12",
    "ts-node": "^10.9.1",
    "typescript": "^4.5.2",
    "vite": "^4.5.9",
    "vite-plugin-static-copy": "^2.2.0"
  },
  "resolutions": {
    "@types/react": "18.2.0",
    "@types/react-native": "0.72.0"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-web": "*"
  },
  "engines": {
    "node": ">= 18"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-ls-commits"
    }
  },
  "commitlint": {
    "extends": [
      "./node_modules/cz-ls-commits/commitlint"
    ]
  },
  "release": {
    "tagFormat": "${version}",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "message": "build(release): v${nextRelease.version} [release]"
        }
      ],
      "@semantic-release/github"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@react-native-community",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "quoteProps": "consistent",
          "singleQuote": true,
          "tabWidth": 2,
          "trailingComma": "es5",
          "useTabs": false
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "module",
        {
          "esm": true,
          "jsxRuntime": "classic"
        }
      ],
      "typescript"
    ]
  },
  "dependencies": {
    "dayjs": "^1.11.13",
    "lodash": "^4.17.21"
  }
}
